generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://localhost:27017/test"
}

type UsersTodoList {
  status Boolean
  text   String
}

model users {
  id       String          @id @default(auto()) @map("_id") @db.ObjectId
  password String
  salt     String
  session  String
  todoList UsersTodoList[]
  username String
}
